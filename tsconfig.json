{
    "compilerOptions": {
        "target": "esNext",
        "module": "esNext",
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "isolatedModules": true,
        "verbatimModuleSyntax": true,
        "erasableSyntaxOnly": false,
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "skipLibCheck": true,
        "noEmit": true,
        "types": ["vite/client"]
    },
    "include": ["src"]
}
